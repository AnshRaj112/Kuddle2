datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Profile {
  bio       String?
  branch    String?
  createdAt DateTime               @default(now())
  id        String                 @id @default(cuid())
  interests EnumProfileInterests[]
  updatedAt DateTime               @updatedAt
  user      User?                  @relation(fields: [userId], references: [id])
  userId    String?
  year      Int?
}

model Like {
  createdAt  DateTime @default(now())
  id         String   @id @default(cuid())
  likedUser  String?
  likeStatus Boolean?
  updatedAt  DateTime @updatedAt
  user       User?    @relation(fields: [userId], references: [id])
  userId     String?
}

model Subscription {
  createdAt        DateTime                          @default(now())
  expiryDate       DateTime?
  id               String                            @id @default(cuid())
  subscriptionType EnumSubscriptionSubscriptionType?
  updatedAt        DateTime                          @updatedAt
  user             User?                             @relation(fields: [userId], references: [id])
  userId           String?
}

model User {
  createdAt     DateTime       @default(now())
  email         String?        @unique
  firstName     String?
  id            String         @id @default(cuid())
  isPremium     Boolean?
  lastName      String?
  likes         Like[]
  password      String
  profiles      Profile[]
  roles         Json
  subscriptions Subscription[]
  updatedAt     DateTime       @updatedAt
  username      String         @unique
}

enum EnumProfileInterests {
  Option1
}

enum EnumSubscriptionSubscriptionType {
  Option1
}
